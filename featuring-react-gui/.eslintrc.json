{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "semi": "error",
    "no-console": "off",
    "react/prop-types": "off"
  },
  "env": {
    "browser": true,
    "mocha": true,
    "es6": true
  },
  "plugins": [
    "react"
  ]
}
